kind: Service
metadata:
  name: Autonomous Compliance-as-Code Engine
  namespace: default
  labels:
    app: Autonomous Compliance-as-Code Engine
spec:
  type: ClusterIP
  ports:
  - port: 80
    targetPort: http
    protocol: TCP
    name: http
  selector:
    app: Autonomous Compliance-as-Code Engine
  sessionAffinity: None
